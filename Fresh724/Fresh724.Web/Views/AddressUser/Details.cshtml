@model Fresh724.Entity.Entities.AddressUser
@{
    ViewData["Title"] = "Details";
}

<form method="post" asp-action="Details" enctype="multipart/form-data">
     <input asp-for="Id" hidden />
     <div class="container py-5">
         <div class="row text-center text-white mb-5">
             <div class="col-lg-7 mx-auto">
                 <h1 class="display-4">Company Details</h1>
             </div>
         </div>
         <div class="row">
             <!-- List group-->
             <ul class="list-group shadow">
                 <!-- list group item-->
                 <li class="list-group-item">
                     <!-- Custom content-->
                     <div class="media-body order-2 order-lg-1">
                     <div class="my-3"><img class="avatar avatar-256 bg-light rounded-circle text-white p-2"
                                                    src="https://raw.githubusercontent.com/twbs/icons/main/icons/person-fill.svg">
                          </div>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="Id"></label> : @Model.Id</h6>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="UserId"></label> : @Model.UserId</h6>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="Street1"></label> : @Model.Street1</h6>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="Street2"></label> : @Model.Street2</h6>
                         <h6 class="font-weight-bold my-2"><label asp-for="City"></label> : @Model.City</h6>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="State"></label> : @Model.State</h6>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="ZipCode"></label> : @Model.ZipCode</h6>
                         <h6 class="mt-0 font-weight-bold mb-2"><label asp-for="Country"></label> : @Model.Country</h6>
                         <ul class="list-inline small">
                             <li class="list-inline-item m-0"><i class="fa fa-star text-success"></i></li>
                             <li class="list-inline-item m-0"><i class="fa fa-star text-success"></i></li>
                             <li class="list-inline-item m-0"><i class="fa fa-star text-success"></i></li>
                             <li class="list-inline-item m-0"><i class="fa fa-star text-success"></i></li>
                         </ul>
                     </div>
                     <div class="d-flex align-items-center justify-content-between mt-1">
                     </div><!-- End -->
                 </li> <!-- End -->

             </ul> <!-- End -->
         </div>
     </div>
     <div class="col-12">
         <a asp-controller="AddressUser" asp-action="Index" class="btn btn-secondary" style="width:150px">Back to List</a>
         </div>
</form>